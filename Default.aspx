<body>
	
	<div class="limiter">
		<div class="container-login100" style="background-image: url('images/bg-01.jpg');">
			<div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">
				<form class="login100-form validate-form" runat="server">
					<span class="login100-form-title p-b-49">
						Login Aplikasi
					</span>

					<div class="wrap-input100 validate-input m-b-23" data-validate = "Username is reauired">
						<span class="label-input100">User Name</span>
                          <asp:TextBox class="input100" type="text" ID="txtUserName" runat="server"  placeholder="Type your username"></asp:TextBox>
						<span class="focus-input100" data-symbol="&#xf206;"></span>
					</div>

					<div class="wrap-input100 validate-input" data-validate="Password is required">
						<span class="label-input100">Password</span>
                          <asp:TextBox class="input100" type="password" ID="txtPassword" runat="server"  placeholder="Type your paswword"></asp:TextBox>
                       <span class="focus-input100" data-symbol="&#xf190;"></span>
					</div>
					
					<div class="text-right p-t-8 p-b-31">
						<div class="forgot-password"><a href="LupaPassword.aspx">Lupa Password</a></div>
					</div>
					
					<div class="container-login100-form-btn">
						<div class="wrap-login100-form-btn">
							<div class="login100-form-bgbtn"></div>
                                     <asp:Button class="btn btn-primary btn-user btn-block" ID="btnSubmit" runat="server" text="LOGIN" OnClick="Login_Click" />
                      <%--               <asp:Button class="login100-form-btn" type="button" ID="btnSubmit" runat="server" text="LOGIN" OnClick="Login_Click" />--%>
						<%--	<button class="login100-form-btn">
